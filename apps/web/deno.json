{
  "name": "@app/web",
  "exports": "./src/main.tsx",
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "generate": "deno run -A npm:orval"
  },
  "imports": {
    "react": "npm:react@19.2.0",
    "react-dom": "npm:react-dom@19.2.0",
    "@types/react": "npm:@types/react@19.2.7",
    "@types/react-dom": "npm:@types/react-dom@19.2.3",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@5.1.1",
    "vite": "npm:vite@7.2.4",
    "@tanstack/react-query": "npm:@tanstack/react-query@5.90.11",
    "react-router-dom": "npm:react-router-dom@7.9.6",
    "tailwindcss": "npm:tailwindcss@4.1.17",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@4.1.17",
    "clsx": "npm:clsx@2.1.1",
    "zod": "npm:zod@4.1.13",
    "orval": "npm:orval@7.17.0"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": ["dom", "dom.iterable", "esnext"],
    "types": ["vite/client"]
  }
}
